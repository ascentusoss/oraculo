{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "paths": {
      "/tipos.js": ["src/tipos/tipos.ts"],
      "/*": ["src/nucleo/*"],
      "/*": ["src/shared/*"],
      "/*": ["src/analistas/*"],
      "/*": ["src/auto/*"],
      "/*": ["src/arquitetos/*"],
      "/*": ["src/zeladores/*"],
      "/*": ["src/relatorios/*"],
      "/*": ["src/guardian/*"],
      "/*": ["src/cli/*"],
      "/*": ["src/tipos/*"],
