      "@/*": ["src/*"],
      "@/resolver.js": ["src/resolver.ts"],
      "@/resolver": ["src/resolver.ts"],
    },
  },
  "include": [
    "src/**/*.ts",
    "tests/fixtures/**/*.ts",
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "vitest.config.ts",
    "exemplos-testes-implementacoes-2/src/**/*.ts",
    "exemplos-testes-implementacoes-2/tests/**/*.ts",
    "exemplos-testes-implementacoes/src/**/*.ts",
