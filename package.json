{
  "name": "oraculo",
  "version": "0.3.7",
  "type": "module",
  "description": "CLI modular para análise, diagnóstico e manutenção de projetos JavaScript/TypeScript",
  "author": "Italo C Lopes",
  "license": "MIT",
  "main": "./dist/bin/cli.js",
  "bin": {
    "oraculo": "./dist/bin/index.js"
  },
  "exports": {
    ".": "./dist/bin/cli.js"
  },
  "engines": {
    "node": ">=24.12.0"
  },
  "scripts": {
    "prebuild": "rimraf dist bin",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "postbuild": "node scripts/copy-artifacts.mjs",
    "prepare": "npm run build",
    "preview": "node scripts/preview.mjs",
    "start": "oraculo",
    "diagnosticar": "oraculo diagnosticar",
    "formatar": "oraculo formatar --write",
    "diagnosticar:json": "oraculo diagnosticar --json",
    "reestruturar": "oraculo reestruturar",
    "podar": "oraculo podar",
    "fix-types": "oraculo fix-types",
    "guardian": "oraculo guardian",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint --ext .ts src",
    "check:style": "npm run lint && npm run typecheck && npm run formatar",
    "test": "vitest run",
    "check": "npm run check:style && npm run coverage && npm run coverage:gate",
    "perf:baseline": "node ./dist/bin/index.js perf baseline --dir docs/perf",
    "perf:compare": "node ./dist/bin/index.js perf compare --dir docs/perf --json",
    "perf:gate": "node ./dist/bin/index.js perf compare --dir docs/perf --json | node -e \"let d='';process.stdin.on('data',c=>d+=c);process.stdin.on('end',()=>{const j=JSON.parse(d);if(j.regressao){console.error('Regressao performance > limite');process.exit(1)}console.log('Perf OK')});\"",
    "sync-aliases": "node scripts/sync-aliases.mjs",
    "validate-aliases": "npm run typecheck && echo 'Aliases válidos!'",
    "licenses:auditar": "node scripts/auditar-licencas.mjs",
    "licenses:notice": "node scripts/generate-notices.mjs",
    "headers:spdx": "node scripts/add-spdx-headers.mjs"
  },
  "keywords": [
    "cli",
    "code-analysis",
    "linter",
    "static-analysis",
    "typescript",
    "javascript",
    "developer-tools",
    "code-quality",
    "architecture",
    "refactoring",
    "monorepo",
    "project-analysis",
    "technical-debt",
    "devtools",
    "maintenance",
    "diagnostics",
    "code-health"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ascentusoss/oraculo.git"
  },
  "bugs": {
    "url": "https://github.com/ascentusoss/oraculo/issues"
  },
  "homepage": "https://github.com/ascentusoss/oraculo#readme",
  "preferGlobal": true,
  "files": [
    "dist",
    "README.md",
    "LICENSE*",
    "THIRD-PARTY-NOTICES.txt",
    "scripts/check-node-version.js"
  ],
  "dependencies": {
    "/parser": "^7.28.6",
    "/traverse": "^7.28.6",
    "/types": "^7.28.6",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "css-tree": "^3.1.0",
    "fast-xml-parser": "^5.3.3",
    "htmlparser2": "^10.1.0",
    "java-parser": "^3.0.1",
    "micromatch": "^4.0.8",
    "ora": "^9.1.0",
    "oraculo": "file:oraculo-0.3.7.tgz",
    "p-limit": "^7.2.0",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "/babel__traverse": "^7.28.0",
    "/jest": "^30.0.0",
    "/micromatch": "^4.0.10",
    "/mocha": "^10.0.10",
    "/node": "^25.0.10",
    "/eslint-plugin": "^8.53.1",
    "/parser": "^8.53.1",
    "/coverage-istanbul": "^4.0.18",
    "/coverage-v8": "^4.0.18",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss-scss": "^4.0.9",
    "rimraf": "^6.1.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "lodash": "4.17.23",
    "lodash-es": "4.17.23"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
